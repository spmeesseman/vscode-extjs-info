{
    "projectName":              "vscode-extjs-info",
    "projectVersion":           "1.7.6",
    "branch":                   "main",
    "buildCommand":             "",
    "buildPostCommand":         [],
    "changelogFile":            "CHANGELOG.md",
    "changelogNoTouch":         true,
    "deployPostCommand":        [
        "git add -- release/vscode-extjs-$(VERSION).vsix CHANGELOG.md README.md .todo \"res/**/*.*\"",
        "git commit -m \"chore: v$(VERSION)\" release/vscode-extjs-$(VERSION).vsix CHANGELOG.md README.md .todo \"res/**/*.*\"",
        "git push origin main:main"
    ],
    "githubAssets": [
        "CHANGELOG.md|Changelog File",
        "release/vscode-extjs-$(VERSION).vsix|VSIX Installer"
    ],
    "githubRelease":            "Y",
    "githubReleasePostCommand": "",
    "githubUser":               "spmeesseman",
    "promptVersion":            "Y",
    "repo":                     "https://github.com/spmeesseman/vscode-extjs-info.git",
    "repoType":                 "git",
    "skipChangelogEdits":       "N",
    "skipVersionEdits":         "Y",
    "vcWebPath":                "https://github.com/spmeesseman/vscode-extjs-info"
}
